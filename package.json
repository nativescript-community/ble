{
  "name": "nativescript-bluetooth",
  "version": "1.0.0",
  "description": "Connect to and interact with Bluetooth LE peripherals",
  "scripts": {
    "tsc": "tsc -skipLibCheck && cp src/bluetooth.d.ts plugin",
    "build": "npm i && npm run tsc",
    "test.android": "npm i && npm run tsc && npm run tslint && cd demo && tns build android && tns test android --justlaunch",
    "test.ios": "npm i && npm run tsc && npm run tslint && cd demo && tns build ios && tns test ios --justlaunch",
    "tslint": "tslint \"src/**/*.ts\" --config tslint.json --exclude \"**/node_modules/**\"",
    "plugin.tscwatch": "npm run tsc -- -w",
    "demo.ios": "npm i && npm run tsc && cd demo && tns run ios",
    "demo.android": "npm i && npm run tsc && cd demo && tns run android",
    "demo.reset": "cd demo && rimraf platforms",
    "plugin.prepare": "npm run tsc && cd demo && tns plugin remove nativescript-bluetooth && tns plugin add ../plugin",
    "clean.demo": "rimraf demo/hooks demo/node_modules demo/platforms",
    "clean": "rimraf plugin/ios plugin/android plugin/**/*.d.ts plugin/**/*.js plugin/**/*.js.map plugin/node_modules plugin/package-lock.json",
    "ci.tslint": "npm i && tslint '**/*.ts' --config '../tslint.json' --exclude '**/node_modules/**'"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/eddyverbruggen/nativescript-bluetooth.git"
  },
  "keywords": [
    "ecosystem:NativeScript",
    "NativeScript",
    "BLE",
    "Bluetooth",
    "Bluetooth LE",
    "Bluetooth Smart",
    "Bluetooth Low Energy"
  ],
  "author": {
    "name": "Eddy Verbruggen",
    "email": "eddyverbruggen@gmail.com"
  },
  "contributors": [
    {
      "name": "Brad Martin",
      "email": "bmartin@nstudio.io",
      "url": "https://github.com/bradmartin"
    }
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/eddyverbruggen/nativescript-bluetooth/issues"
  },
  "homepage": "https://github.com/eddyverbruggen/nativescript-bluetooth",
  "devDependencies": {
    "@types/node": "^10.14.8",
    "esdoc": "^1.1.0",
    "esdoc-ecmascript-proposal-plugin": "^1.0.0",
    "esdoc-publish-markdown-plugin": "^1.1.0",
    "esdoc-standard-plugin": "^1.0.0",
    "esdoc-typescript-plugin": "^1.0.1",
    "prettier": "^1.17.1",
    "rimraf": "^2.6.3",
    "tns-core-modules": "^5.4.1",
    "tns-platform-declarations": "^5.4.1",
    "tslint": "^5.17.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-config-standard": "^8.0.1",
    "typescript": "~3.5.1"
  },
  "dependencies": {
    "nativescript-observable": "^1.0.1",
    "p-queue": "^5.0.0"
  }
}
